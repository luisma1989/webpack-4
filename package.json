{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Proyecto con la configutación de un proyecto javascript con webpack 4",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:devServer": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --watch",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js --watch",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js --watch",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js --watch",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisma1989/webpack-4.git"
  },
  "keywords": [],
  "author": "Luis Manuel Fernández Camacho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luisma1989/webpack-4/issues"
  },
  "homepage": "https://github.com/luisma1989/webpack-4#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "postcss": "^7.0.26",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
